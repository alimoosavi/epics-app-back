record(calc, "Time_Count")
{
 field(CALC, "A=1 ? B+1 : C")
 field(SCAN, "1 second")
 field(INPA, "Alarm.VAL")
 field(INPB, "Time_Count0.VAL")
 field(INPC, "Time_Count0.VAL")
 field(FLNK, "Power_Usage")
}

record(calc, "Time_Count0")
{
 field(CALC, "A+0")
 field(SCAN, ".1 second")
 field(INPA, "Time_Count.VAL")
}

record(ai, "PowerPH")
{
 field(DESC, "The Chiller Device Power Usage Per Hour")
}

record(ao, "Power_Usage_Display")
{
 field(DOL, "Power_Usage")
 field(SCAN, "Passive")
 field(OMSL, "closed_loop")
}

record(calc, "Power_Usage")
{
 field(CALC, "(A/3600)*B")
 field(SCAN, "Passive")
 field(INPA, "Time_Count.VAL")
 field(INPB, "PowerPH.VAL")
 field(FLNK, "Power_Usage_Display")
}